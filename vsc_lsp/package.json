{
    "name": "vecscore-vsc",
    "displayName": "VecScoreDrawer LSP",
    "publisher": "your-publisher-name",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "vecscore",
                "aliases": [
                    "VecScore",
                    "vsc"
                ],
                "extensions": [
                    ".vsc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "commands": [
            {
                "command": "vecscore.startServer",
                "title": "Start VecScore LSP Server"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./"
    },
    "dependencies": {
        "vscode": "^1.1.37",
        "vscode-languageclient": "^9.0.1"
    },
    "devDependencies": {
        "@types/node": "^22.14.1",
        "typescript": "^5.8.3"
    }
}
